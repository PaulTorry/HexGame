{


    "extends":         "eslint:recommended",

    "env": {           "browser": true       },

    "parserOptions": {        "ecmaVersion": 8      },

      "rules": {
         "eqeqeq": 1 ,
         "no-console": 0,
         "strict": 0,
         "padded-blocks": 0,
         "require-jsdoc": 0,
         "func-style": 0,
         "no-implicit-globals":0,
         "no-magic-numbers":0,
         "indent": ["error", 2]
       }


}
